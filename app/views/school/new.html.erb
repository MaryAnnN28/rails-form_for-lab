<h1>School Class Form</h1>
<br>

<%= form_for(@school_class) do |school_class| %>
     <label>Title: </label><br>
     <%= school_class.text_field :title %><br>

     <label>Room Number: </label><br>
     <%= school_class.text_field :room_number %><br>

     <label>Create School class</label><br>
     <%= school_class.submit %>

<% end %>